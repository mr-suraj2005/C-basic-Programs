#include <stdio.h>
void main()
{
    int arr[10]={1,2,3,4};
    int newE=5,size=4;
    printf("Arrays before insertion of elements\n");
    for(int i=0;i<size;i++){
        printf("%d",arr[i]);
    }
    printf("\n");
    arr[size]=newE;
    size++;
    printf("Arrays after insertion of elements\n");
    for(int i=0;i<size;i++){
        printf("%d",arr[i]);
    }
}
