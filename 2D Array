#include<stdio.h>
void main(){
    int rows,cols;
    printf("Enter number of rows = ");
    scanf("%d",&rows);
    printf("Enter number of columns = ");
    scanf("%d",&cols);
    int arr[rows][cols];
    printf("Number of rows and columns are = %d\t%d\n",rows,cols);
    for(int i=0;i<rows;i++) {
        for(int j=0;j<cols;j++){
            printf("Elements [%d][%d] = " ,i,j);
            scanf("%d" ,& arr[i][j]);
        }
    }
}
