#include<stdio.h>
void main()
{
    int i,sum=0,n;
    printf("Enter number of terms: \n");
    scanf("%d" ,&n);
    int arr[n];
    printf("Numbers are= ");
    for(i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    for(i=0;i<n;i++){
        sum+=arr[i];
    }
    printf("The numbers are= %d\n",sum);
}
